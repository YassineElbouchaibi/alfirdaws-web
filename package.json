{
  "name": "alfirdaws-web",
  "private": true,
  "version": "0.1.2",
  "scripts": {
    "apollo:generate": "yarn apollo codegen:generate --target=typescript --tagName=gql --includes='operations/**/types.ts' --endpoint=http://localhost:1338/graphql --header='apollocodegen: true' --globalTypesFile=types/globalTypes.ts",
    "develop": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "yarn husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "format:eslint": "yarn eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "format:prettier": "yarn prettier --write",
    "format": "yarn format:eslint && yarn format:prettier ."
  },
  "dependencies": {
    "@apollo/client": "^3.5.8",
    "@emotion/cache": "^11.7.1",
    "@emotion/react": "^11.7.1",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.6.0",
    "@fontsource/source-sans-pro": "^4.5.3",
    "@mui/icons-material": "^5.3.1",
    "@mui/material": "^5.3.1",
    "date-fns": "^2.28.0",
    "graphql": "^15",
    "next": "12.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.2",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-interactions": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@storybook/testing-library": "^0.0.9",
    "@types/node": "17.0.13",
    "@types/react": "17.0.38",
    "apollo": "2.33.9",
    "babel-loader": "^8.2.3",
    "eslint": "^8.9.0",
    "eslint-config-next": "13.0.3",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-storybook": "^0.5.7",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "prettier": "2.5.1",
    "pretty-quick": "3.1.3",
    "typescript": "4.5.5"
  },
  "resolutions": {
    "graphql": "^15"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "yarn eslint --cache --fix"
  }
}